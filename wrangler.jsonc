{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "ratemybroker",
	"main": "src/index.ts",
	"compatibility_date": "2025-05-28",
	"compatibility_flags": [
		"global_fetch_strictly_public"
	],
	"assets": {
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
	"durable_objects": {
		"bindings": [
			{ "name": "BROKER_STORE", "class_name": "BrokerDataStore" }
		]
	},
	"migrations": [
		{ "tag": "v1", "new_classes": ["BrokerDataStore"] }
	]
}
